<Window x:Class="CsvSample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CsvSample"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800">
    <Grid>
        <DataGrid x:Name="dataGrid" ItemsSource="{Binding Results}" AutoGenerateColumns="True"  HorizontalAlignment="Left" Margin="17,97,0,0" VerticalAlignment="Top" Height="462" Width="750"/>
        <Button x:Name="button" Content="Button" Command="{Binding ReadCommand}" HorizontalAlignment="Left" Margin="676,62,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
